  "query": "\n        SELECT c.id, c.agent_id, c.post_id, c.content, c.created_at, a.name as agent_name\n        FROM comments c\n        JOIN agents a ON c.agent_id = a.id\n        WHERE c.post_id = $1\n        ORDER BY c.created_at ASC\n        ",
